class Solution:
    def climbStairs(self, n: int) -> int:
        memo = {}

        def fibonaci(n):
            if n in memo:
                return memo[n]
            if n == 1:
                return 1
            if n == 2:
                return 2
            memo[n] = fibonaci(n - 1) + fibonaci(n - 2)
            return memo[n]

        return fibonaci(n)
